    "primeicons": "^7.0.0",
    "primereact": "^10.9.7",
    "react": "^19.2.0",
    "react-bootstrap-icons": "^1.11.6",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
